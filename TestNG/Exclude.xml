<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Exclude test cases">
    <test name="Low priority">
        <groups>
            <run>
                <include name="Regression"/>
                <exclude name="Broken"/>
            </run>
        </groups>
        <classes>
            <class name="TestNG.Grouping"></class>
        </classes>
    </test>
</suite>