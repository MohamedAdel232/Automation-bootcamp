<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Group">
    <test name="Regression testing">
        <groups>
            <define name="Function">
                <include name="Regression"/>
                <include name="Smoke"/>
            </define>
            <run>
                <include name="Function"/>
            </run>
        </groups>
        <classes>
            <class name="TestNG.Grouping"/>
        </classes>
    </test>
</suite>